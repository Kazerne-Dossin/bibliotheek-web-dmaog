PREFIX : <http://example.com/>
PREFIX schema: <http://schema.org/>
PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX dbr: <http://dbpedia.org/resource/>
PREFIX kdbib: <http://bibliotheek.kazernedossin.eu/books/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
SOURCE books_db_preciosa <jdbc:ucanaccess://C:\Users\Herminio\Desktop\dmaog-playground\BIBLIOTHEEK_2025_Preciosa.mdb;memory=false>

ITERATOR books_preciosa <sql: SELECT * FROM [Preciosa-Boeken];> {
    FIELD id <Boek-ID>
    FIELD author <Auteur>
    FIELD title <Titel>
    FIELD bookEditorial <Uitgeverij>
    FIELD bookEditorialPlace <Plaats>
    FIELD editionDate <Datum>
    FIELD language <Taal>
    FIELD keywords <Keywords>
    FIELD donatedBy <Schenker>
    FIELD info <Opmerkingen>
    FIELD ISBN <ISBN>
    FIELD cdrom <cd-rom>
    FIELD numberOfPages <Pagina's>
}

MATCHER booleans <FALSE AS false & TRUE AS true>

EXPRESSION book <books_db_preciosa.books_preciosa>

schema:Book kdbib:[book.id] {
    a dc:Text ;
    dc:title [book.title] ;
    dc:creator [book.author] ;
    dc:identifier [book.ISBN] xsd:string ;
    dc:publisher [book.bookEditorial] ;
    schema:locationCreated [book.bookEditorialPlace] ;
    dc:created dbr:[book.editionDate] ;
    dc:language dbr:[book.language] ;
    :info [book.info] ;
    #:cdrom [book.cdrom MATCHING booleans] ;
    #dc:hasVersion [book.doubleOrVersion] ;
    dc:sizeOrDuration [book.numberOfPages] ;
    :donatedBy [book.donatedBy] ;
    #:missing [book.missing MATCHING booleans] ;
    dc:subject [book.keywords] ;
    #:lent [book.lent] ;
    #:lendingDate [book.lendingDate] ;
}
